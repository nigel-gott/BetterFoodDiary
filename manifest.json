{
    "name": "Better Food Diary",
    "version": "0.0.0.1",
    "description": "Improves the myfitnesspal.com food diary.",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "http://www.myfitnesspal.com/food/diary/*"
            ],
            "js": ["scripts/jquery.js", "scripts/bfd.js"]
        }
    ],
    "permissions": [
        "tabs",
        "unlimitedStorage",
        "storage"
    ]
}
